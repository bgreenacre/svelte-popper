{
	"name": "svelte-popper",
	"description": "This is a simple svelte component that is a wrapper for popper.js",
	"author": "Brian Greenacre <bgreenacre42@gmail.com>",
	"license": "MIT",
	"version": "0.1.2",
	"repository": {
		"type": "git",
		"url": "git@github.com:bgreenacre/svelte-popper.git"
	},
	"bugs": {
		"url": "https://github.com/bgreenacre/svelte-popper/issues"
	},
	"svelte": "src/components/components.module.js",
	"module": "dist/index.min.mjs",
	"main": "dist/index.min.js",
	"devDependencies": {
		"@babel/core": "7.6.4",
		"@babel/plugin-proposal-class-properties": "7.7.0",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.6.0",
		"@babel/preset-env": "7.6.3",
		"@testing-library/jest-dom": "4.2.3",
		"@testing-library/svelte": "1.9.0",
		"@types/jest": "24.0.21",
		"autoprefixer": "9.7.1",
		"babel-jest": "24.9.0",
		"eslint": "6.6.0",
		"eslint-config-prettier": "6.5.0",
		"eslint-plugin-jest": "23.0.2",
		"eslint-plugin-svelte3": "2.7.3",
		"husky": "3.0.9",
		"jest": "24.9.0",
		"lint-staged": "9.4.2",
		"npm-run-all": "4.1.5",
		"postcss": "7.0.21",
		"postcss-load-config": "2.1.0",
		"prettier": "1.18.2",
		"rollup": "1.26.3",
		"rollup-plugin-babel": "4.3.3",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-livereload": "1.0.4",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-svelte": "5.1.0",
		"rollup-plugin-terser": "5.1.2",
		"sirv-cli": "0.4.5",
		"svelte": "3.12.1",
		"svelte-jest": "0.3.1",
		"svelte-preprocess": "3.2.2"
	},
	"scripts": {
		"lint": "eslint --color .",
		"test": "jest",
		"autobuild": "rollup -c -w",
		"start:dev": "sirv public --single --dev",
		"dev": "run-p start:dev autobuild",
		"build": "rollup -c",
		"prepublishOnly": "npm run build"
	},
	"browserslist": [
		"defaults"
	],
	"keywords": [
		"svelte",
		"svelte-popper",
		"popperjs",
		"component",
		"popover"
	],
	"files": [
		"src",
		"dist"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js, svelte}": [
			"eslint --fix",
			"git add"
		],
		"*.{html, css, scss, stylus, js, ts, json, yml, md}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"classnames": "2.2.6",
		"popper.js": "1.16.0"
	}
}
